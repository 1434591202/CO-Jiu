
.topImg>image{
	width: 100%;
	height: 600rpx;
	z-index: -1;
}
.graD{
	width: 350rpx;
	height: 50rpx;
	padding: 15rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	position: absolute;
	line-height: 50rpx;
	top:350rpx;
	left: 50rpx;
	padding-left:90rpx;
	border-radius: 25rpx;
	background-image: url(data:image/png;base64,);
	background-color: rgba(243,243,243,0.4);
	background-repeat: no-repeat;
	background-size: 50rpx 50rpx;
	background-position: 30rpx 16rpx ;
}
.graD>view{
	text-align: center;
}
.graD>view:nth-child(2){
	width: 220rpx;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 50rpx 50rpx;
	background-position:10rpx -3rpx ;
}
.graD>view:nth-child(1){
	width: 150rpx;
	margin-right: 15rpx;
	border-right: #000 solid 5rpx;
}
.baseIBox{
	width: 100%;
	height: 500rpx;
	background-color: #fff;
	border-top-left-radius:25rpx ;
	border-top-right-radius:25rpx ;
	margin-top: -100rpx;
}


